void setup() {
  
  int pin = 12;
  int weirdB = 247;
  int C = 262;
  int D = 294;
  int E = 330;
  int F = 349;
  int G = 392;
  int A = 440;
  int B = 494;
  int C5 = 523;
  int beat1 = 450;
  int beat2 = beat1 * 2;
  int beat3 = beat1 * 3;
  int beat6 = beat1 * 6;

//an-oth-er head-
  
  tone(pin, A);
  delay(beat1);
  noTone(pin);

  tone(pin, G);
  delay(beat2);
  noTone(pin);

